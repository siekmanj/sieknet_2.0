[network]
name: recurrent_model
input_dimension: 40
input: input_embedding
output: action

[fully_connected layer]
name: input_embedding
size: 64
logistic: tanh

[lstm layer]
input: input_embedding
name: prediction
size: 40

[lstm layer]
input: input_embedding, prediction
name: action
size: 10

[lstm layer]
input: input_embedding, prediction
name: reward_prediction
size: 1
